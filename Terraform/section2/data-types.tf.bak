variable "my-list" {
  # "any" is a data type, such as number, string and so on
  type = list(any)
}

output "variable_value" {
  value = var.my-list
}

# security_group_ids should use list
/*
resource "aws_instance" "web" {
  ami = "ami-123"
  instance_type = "t3.micro"
  vpc_security_group_ids = [ "sg-1234" ]
}
*/

variable "list" {
  type    = list(string)
  default = ["m5.large", "m5.xlarge", "t2.medium"]
}

variable "types" {
  type = map(string)
  default = {
    "us-east-1"  = "t2.micro"
    "us-west-2"  = "t2.nano"
    "us-south-1" = "t2.small"
  }
}

